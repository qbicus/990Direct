{
  "name": "990 Direct",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Gets you the direct link to 990.ro tv series/films",
  "browser_action": {
    "name": "990 Direct",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "http://*/"
  ],
  "content_scripts": [ {
    "js": [ "990Direct.js" ],
    "css": [ "buttons.css" ],
    "matches": [ "http://www.990.ro/*", "http://superweb.rol.ro/*" ]
  }]
}

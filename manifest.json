{
  "name": "990 Direct",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Gets you the direct link to 990 tvseries",
  "browser_action": {
    "name": "990 Direct",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "990Direct.js" ],
    "matches": ["http://www.990.ro/*", "http://superweb.rol.ro/*"]
  }]
}